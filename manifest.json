{
    "name": "BlackRhino",
    "description": "Browse the Internet and Earn Nano",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {"128": "/imgs/black-rhino-icon.png"},
    "permissions": [
        "storage",
        "webRequest",
        "tabs"
    ],
    "host_permissions": [
        "http://blackrhino-ce.com/*"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    }
}